{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "key-data.schema.json",
    "$ref": "#/definitions/data",
    "definitions": {
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "padLeft": {
                    "type": "string"
                },
                "padRight": {
                    "type": "string"
                },
                "ignoreIfUndefined": {
                    "type": "boolean"
                },
                "ignoreIfUndefinedReplacement": {
                    "type": "string"
                },
                "switch": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": [
                        "cases"
                    ],
                    "properties": {
                        "cases": {
                            "type": "object",
                            "additionalProperties": false,
                            "patternProperties": {
                                ".*": {
                                    "type": "string"
                                }
                            }
                        },
                        "default": {
                            "type": "string"
                        }
                    }
                },
                "arrayJoin": {
                    "type": "string"
                }
            }
        }
    }
}